    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP
);


ALTER TABLE public.review_history OWNER TO leetcodeuser;

--
-- TOC entry 251 (class 1259 OID 59746)
-- Name: problem_stats; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.problem_stats AS
 SELECT p.id,
    p.problem_id,
    p.title,
    p.difficulty,
    count(rh.id) AS total_reviews,
    count(
        CASE
            WHEN (rh.result = 'remembered'::public.review_result) THEN 1
            ELSE NULL::integer
        END) AS successful_reviews,
    count(
        CASE
            WHEN (rh.result = 'forgot'::public.review_result) THEN 1
            ELSE NULL::integer
        END) AS failed_reviews,
        CASE
            WHEN (count(rh.id) > 0) THEN round((((count(
            CASE
                WHEN (rh.result = 'remembered'::public.review_result) THEN 1
                ELSE NULL::integer
            END))::numeric * 100.0) / (count(rh.id))::numeric), 2)
            ELSE (0)::numeric
        END AS success_rate,
    max(rh.review_date) AS last_reviewed,
    min(rh.next_review_date) AS next_review
   FROM (public.problems p
     LEFT JOIN public.review_history rh ON ((p.id = rh.problem_id)))
  GROUP BY p.id, p.problem_id, p.title, p.difficulty;


ALTER VIEW public.problem_stats OWNER TO leetcodeuser;

--
-- TOC entry 241 (class 1259 OID 59624)
-- Name: problem_tags; Type: TABLE; Schema: public; Owner: leetcodeuser

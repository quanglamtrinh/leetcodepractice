ing","attrs":{"level":2},"content":[{"type":"text","text":" Why It Works"}]},{"type":"heading","attrs":{"level":3},"content":[{"type":"text","marks":[{"type":"bold"}],"text":"Using stacks simulates right-to-left addition"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"By pushing all digits, the top of each stack represents the least significant digit."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Each pop operation processes one digit from the end of both numbers, perfectly simulating addition from right to left."}]}]}]},{"type":"heading","attrs":{"level":3},"content":[{"type":"text","marks":[{"type":"bold"}],"text":"Building the result from the front"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Each newly created node is added "},{"type":"text","marks":[{"type":"bold"}],"text":"to the front"},{"type":"text","text":" ("},{"type":"text","marks":[{"type":"code"}],"text":"node.next = head"},{"type":"text","text":"),"},{"type":"hardBreak"},{"type":"text","text":"so the final list is automatically in correct forward order."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Example Walkthrough"}]},{"type":"heading","attrs":{"level":3},"content":[{"type":"text","text":"Input:"}]},{"type":"codeBlock","attrs":{"language":"python"},"content":[{"type":"text","text":"l1 = [7,2,4,3]\\nl2 = [5,6,4]\\n"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Stacks:"}]},{"type":"codeBlock","attrs":{"language":"python"},"content":[{"type":"text","text":"stack1 = [7,2,4,3]\\nstack2 = [5,6,4]\\n"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Computation:"}]},{"type":"table","content":[{"type":"tableRow","content":[{"type":"tableHeader","attrs":{"colspan":1,"rowspan":1,"colwidth"
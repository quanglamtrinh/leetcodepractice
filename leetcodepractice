CREATE TABLE public.problems (
    id bigint NOT NULL,
    problem_id bigint NOT NULL,
    title character varying(255) NOT NULL,
    concept character varying(100),
    difficulty public.difficulty_level NOT NULL,
    acceptance_rate numeric(5,2),
    popularity bigint,
    solved boolean DEFAULT false,
    notes text,
    leetcode_link text,
    solution text,
    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    updated_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    similar_problems bigint[] DEFAULT '{}'::bigint[]
);


ALTER TABLE public.problems OWNER TO leetcodeuser;

--
-- TOC entry 252 (class 1259 OID 59751)
-- Name: mistake_analysis; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.mistake_analysis AS
 SELECT p.title,
    p.difficulty,
    m.mistake_type,
    count(*) AS mistake_count,
    string_agg(m.description, '; '::text) AS mistake_descriptions
   FROM (public.problems p
     JOIN public.mistakes m ON ((p.id = m.problem_id)))
  GROUP BY p.id, p.title, p.difficulty, m.mistake_type
  ORDER BY (count(*)) DESC;


ALTER VIEW public.mistake_analysis OWNER TO leetcodeuser;


    CONSTRAINT problem_tags_check CHECK (((((((((problem_id IS NOT NULL))::integer + ((variant_id IS NOT NULL))::integer) + ((pattern_id IS NOT NULL))::integer) + ((goal_id IS NOT NULL))::integer) + ((technique_id IS NOT NULL))::integer) + ((concept_id IS NOT NULL))::integer) >= 2))
);


ALTER TABLE public.problem_tags OWNER TO leetcodeuser;


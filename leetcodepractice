--
-- TOC entry 261 (class 1259 OID 59901)
-- Name: pattern_analytics; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.pattern_analytics AS
 SELECT p.id AS pattern_id,
    p.name AS pattern_name,
    p.description AS pattern_description,
    c.name AS concept_name,
    c.concept_id AS concept_identifier,
    pp.total_attempts,
    pp.successful_attempts,
        CASE
            WHEN (pp.total_attempts > 0) THEN round((((pp.successful_attempts)::numeric * 100.0) / (pp.total_attempts)::numeric), 2)
            ELSE (0)::numeric
        END AS success_rate,
    pp.average_time_minutes,
    pp.difficulty_rating,
    pp.last_reviewed,
    count(pa.id) AS associated_problems_count,
    count(v.id) AS variants_count
   FROM ((((public.patterns p
     LEFT JOIN public.concepts c ON ((p.concept_id = c.id)))
     LEFT JOIN public.pattern_performance pp ON ((p.id = pp.pattern_id)))
     LEFT JOIN public.problem_associations pa ON ((p.id = pa.pattern_id)))
     LEFT JOIN public.variants v ON ((p.id = v.pattern_id)))
  GROUP BY p.id, p.name, p.description, c.name, c.concept_id, pp.total_attempts, pp.successful_attempts, pp.average_time_minutes, pp.difficulty_rating, pp.last_reviewed
  ORDER BY pp.difficulty_rating DESC NULLS LAST, p.name;


ALTER VIEW public.pattern_analytics OWNER TO leetcodeuser;

--
-- TOC entry 5343 (class 0 OID 0)
-- Dependencies: 261
-- Name: VIEW pattern_analytics; Type: COMMENT; Schema: public; Owner: leetcodeuser
--

COMMENT ON VIEW public.pattern_analytics IS 'Comprehensive analytics view for pattern performance';



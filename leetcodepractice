--
-- TOC entry 291 (class 1255 OID 59739)
-- Name: get_due_problems_today(); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_due_problems_today() RETURNS TABLE(id bigint, problem_id bigint, title character varying, difficulty public.difficulty_level, next_review_date date, days_overdue integer)
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    SELECT 
        p.id,
        p.problem_id,
        p.title,
        p.difficulty,
        rh.next_review_date,
        (CURRENT_DATE - rh.next_review_date)::INTEGER as days_overdue
    FROM problems p
    INNER JOIN (
        SELECT DISTINCT ON (review_history.problem_id) 
            review_history.problem_id, 
            review_history.next_review_date
        FROM review_history 
        ORDER BY review_history.problem_id, review_history.created_at DESC
    ) rh ON p.id = rh.problem_id
    WHERE rh.next_review_date <= CURRENT_DATE
    ORDER BY rh.next_review_date ASC, p.difficulty DESC;
END;
$$;


ALTER FUNCTION public.get_due_problems_today() OWNER TO leetcodeuser;

--
-- TOC entry 299 (class 1255 OID 61314)
-- Name: get_events_for_day(date); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_events_for_day(p_date date) RETURNS TABLE(id bigint, event_type public.event_type, title character varying, event_time time without time zone, task_status public.task_status, priority public.event_priority, color character varying, problem_difficulty public.difficulty_level)
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    SELECT 
        ce.id,
        ce.event_type,
        ce.title,
        ce.event_time,
        ce.task_status,
        ce.priority,
        ce.color,
        p.difficulty as problem_difficulty
    FROM calendar_events ce
    LEFT JOIN problems p ON ce.problem_id = p.id
    WHERE ce.event_date = p_date
        AND ce.is_archived = FALSE
        AND ce.is_visible = TRUE
    ORDER BY ce.event_time ASC NULLS LAST, ce.priority DESC;
END;
$$;


ALTER FUNCTION public.get_events_for_day(p_date date) OWNER TO leetcodeuser;

--
-- TOC entry 302 (class 1255 OID 61315)
-- Name: get_overdue_tasks(); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_overdue_tasks() RETURNS TABLE(id bigint, title character varying, due_date date, priority public.event_priority, days_overdue integer)
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    SELECT 
        ce.id,
        ce.title,
        ce.due_date,
        ce.priority,
        (CURRENT_DATE - ce.due_date)::INTEGER as days_overdue
    FROM calendar_events ce
    WHERE ce.event_type = 'task'
        AND ce.task_status NOT IN ('completed', 'cancelled')
        AND ce.due_date < CURRENT_DATE
        AND ce.is_archived = FALSE
    ORDER BY ce.due_date ASC, ce.priority DESC;
END;
$$;


ALTER FUNCTION public.get_overdue_tasks() OWNER TO leetcodeuser;

--
-- TOC entry 286 (class 1255 OID 49898)
-- Name: get_problem_review_history(integer); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_problem_review_history(problem_id_param integer) RETURNS TABLE(problem_name character varying, difficulty character varying, total_attempts bigint, remembered_count bigint, forgot_count bigint, success_rate numeric, review_dates date[], next_planned_reviews date[])
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    WITH problem_stats AS (
        SELECT 
            p.title,
            p.difficulty,
            COUNT(rh.id) as total_attempts,
            COUNT(CASE WHEN rh.result = 'remembered' THEN 1 END) as remembered_count,
            COUNT(CASE WHEN rh.result = 'forgot' THEN 1 END) as forgot_count,
            CASE 
                WHEN COUNT(rh.id) > 0 THEN 
                    ROUND((COUNT(CASE WHEN rh.result = 'remembered' THEN 1 END) * 100.0 / COUNT(rh.id)), 2)
                ELSE 0
            END as success_rate,
            ARRAY_AGG(rh.review_date ORDER BY rh.review_date) FILTER (WHERE rh.review_date IS NOT NULL) as review_dates
        FROM problems p
        LEFT JOIN review_history rh ON p.id = rh.problem_id
        WHERE p.id = problem_id_param
        GROUP BY p.id, p.title, p.difficulty
    ),
    future_reviews AS (
        SELECT ARRAY_AGG(future_date ORDER BY future_date) as next_reviews
        FROM (
            SELECT 
                p.next_review_date + (interval '1 day' * generate_series(0, 2)) as future_date
            FROM problems p
            WHERE p.id = problem_id_param 
            AND p.next_review_date IS NOT NULL
            AND p.next_review_date >= CURRENT_DATE
            LIMIT 3
        ) dates
    )
    SELECT 
        ps.title,
        ps.difficulty,
        ps.total_attempts,
        ps.remembered_count,
        ps.forgot_count,
        ps.success_rate,
        ps.review_dates,
        COALESCE(fr.next_reviews, ARRAY[]::DATE[])
    FROM problem_stats ps
    CROSS JOIN future_reviews fr;
END;
$$;


ALTER FUNCTION public.get_problem_review_history(problem_id_param integer) OWNER TO leetcodeuser;

--
-- TOC entry 285 (class 1255 OID 49756)
-- Name: get_problems_by_pattern(text); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_problems_by_pattern(pattern_name_param text) RETURNS TABLE(problem_title character varying, difficulty character varying, approach_type text, approach_name text, is_primary boolean, difficulty_rating integer, leetcode_link text)
    LANGUAGE plpgsql
    AS $$
      BEGIN
        RETURN QUERY
        SELECT 
          prob.title,
          prob.difficulty,
          CASE 
            WHEN pp.pattern_id IS NOT NULL THEN 'Core Pattern'
            ELSE 'Variant'
          END as approach_type,
          COALESCE(p.name, pv.name) as approach_name,
          pp.is_primary_approach,
          pp.difficulty_rating,
          prob.leetcode_link
        FROM problems prob
        JOIN problem_patterns pp ON prob.id = pp.problem_id
        LEFT JOIN patterns p ON pp.pattern_id = p.id
        LEFT JOIN pattern_variants pv ON pp.variant_id = pv.id
        LEFT JOIN patterns base_p ON pv.base_pattern_id = base_p.id
        WHERE p.name = pattern_name_param OR base_p.name = pattern_name_param
        ORDER BY prob.difficulty, pp.difficulty_rating;
      END;
      $$;


ALTER FUNCTION public.get_problems_by_pattern(pattern_name_param text) OWNER TO leetcodeuser;

--
-- TOC entry 288 (class 1255 OID 49900)
-- Name: get_problems_due_for_review(); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_problems_due_for_review() RETURNS TABLE(id integer, title character varying, difficulty character varying, next_review_date date, days_overdue integer)
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    SELECT 
        p.id,
        p.title,
        p.difficulty,
        p.next_review_date,
        (CURRENT_DATE - p.next_review_date)::INTEGER as days_overdue
    FROM problems p
    WHERE p.in_review_cycle = TRUE 
    AND p.next_review_date <= CURRENT_DATE
    ORDER BY p.next_review_date ASC, p.difficulty DESC;
END;
$$;


ALTER FUNCTION public.get_problems_due_for_review() OWNER TO leetcodeuser;

--
-- TOC entry 297 (class 1255 OID 59918)
-- Name: get_recommended_focus_areas(bigint, integer); Type: FUNCTION; Schema: public; Owner: leetcodeuser
--

CREATE FUNCTION public.get_recommended_focus_areas(p_user_id bigint DEFAULT 1, p_limit integer DEFAULT 5) RETURNS TABLE(type text, id bigint, name text, difficulty_rating numeric, success_rate numeric, days_since_review integer, priority_score numeric)
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    WITH pattern_priorities AS (
        SELECT 
            'pattern'::TEXT as type,
            p.id,
            p.name,
            pp.difficulty_rating,
            CASE WHEN pp.total_attempts > 0 THEN pp.successful_attempts::DECIMAL / pp.total_attempts * 100 ELSE 0 END as success_rate,
            COALESCE(CURRENT_DATE - pp.last_reviewed, 999) as days_since_review,
            -- Priority score: higher difficulty + lower success rate + more days = higher priority
            (COALESCE(pp.difficulty_rating, 0.5) * 0.4 + 
             (1.0 - COALESCE(pp.successful_attempts::DECIMAL / NULLIF(pp.total_attempts, 0), 0.5)) * 0.4 +
             LEAST(COALESCE(CURRENT_DATE - pp.last_reviewed, 30), 30) / 30.0 * 0.2) as priority_score
        FROM patterns p
        LEFT JOIN pattern_performance pp ON p.id = pp.pattern_id AND pp.user_id = p_user_id
    ),
    variant_priorities AS (
        SELECT 
            'variant'::TEXT as type,
            v.id,
            v.name,
            vp.difficulty_rating,
            CASE WHEN vp.total_attempts > 0 THEN vp.successful_attempts::DECIMAL / vp.total_attempts * 100 ELSE 0 END as success_rate,
            COALESCE(CURRENT_DATE - vp.last_reviewed, 999) as days_since_review,
            -- Priority score: higher difficulty + lower success rate + more days = higher priority
            (COALESCE(vp.difficulty_rating, 0.5) * 0.4 + 
             (1.0 - COALESCE(vp.successful_attempts::DECIMAL / NULLIF(vp.total_attempts, 0), 0.5)) * 0.4 +
             LEAST(COALESCE(CURRENT_DATE - vp.last_reviewed, 30), 30) / 30.0 * 0.2) as priority_score
        FROM variants v
        LEFT JOIN variant_performance vp ON v.id = vp.variant_id AND vp.user_id = p_user_id
    )
    SELECT * FROM (
        SELECT * FROM pattern_priorities
        UNION ALL
        SELECT * FROM variant_priorities
    ) combined
    ORDER BY priority_score DESC
    LIMIT p_limit;
END;
$$;


ALTER FUNCTION public.get_recommended_focus_areas(p_user_id bigint, p_limit integer) OWNER TO leetcodeuser;


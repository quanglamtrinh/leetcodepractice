--
-- TOC entry 271 (class 1259 OID 61330)
-- Name: calendar_monthly_overview; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.calendar_monthly_overview AS
 SELECT date_trunc('month'::text, (event_date)::timestamp with time zone) AS month,
    count(*) AS total_events,
    count(
        CASE
            WHEN (event_type = 'task'::public.event_type) THEN 1
            ELSE NULL::integer
        END) AS tasks,
    count(
        CASE
            WHEN (event_type = 'note'::public.event_type) THEN 1
            ELSE NULL::integer
        END) AS notes,
    count(
        CASE
            WHEN (event_type = 'practice_session'::public.event_type) THEN 1
            ELSE NULL::integer
        END) AS practice_sessions,
    count(DISTINCT event_date) AS active_days
   FROM public.calendar_events
  WHERE (is_archived = false)
  GROUP BY (date_trunc('month'::text, (event_date)::timestamp with time zone))
  ORDER BY (date_trunc('month'::text, (event_date)::timestamp with time zone)) DESC;


ALTER VIEW public.calendar_monthly_overview OWNER TO leetcodeuser;


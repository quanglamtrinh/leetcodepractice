t","marks":[{"type":"code"}],"text":"i % n"},{"type":"text","text":" to get the current index in "},{"type":"text","marks":[{"type":"code"}],"text":"nums"},{"type":"text","text":"."}]},{"type":"paragraph","content":[{"type":"text","text":"For each index "},{"type":"text","marks":[{"type":"code"}],"text":"i"},{"type":"text","text":":"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"While the stack is not empty and "},{"type":"text","marks":[{"type":"code"}],"text":"nums[i % n] > nums[stack[-1]]"},{"type":"text","text":","},{"type":"hardBreak"},{"type":"text","text":" Pop "},{"type":"text","marks":[{"type":"code"}],"text":"prev"},{"type":"text","text":" from the stack."},{"type":"hardBreak"},{"type":"text","text":" Set "},{"type":"text","marks":[{"type":"code"}],"text":"ans[prev] = nums[i % n]"},{"type":"text","text":"."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"If "},{"type":"text","marks":[{"type":"code"}],"text":"i < n"},{"type":"text","text":", push "},{"type":"text","marks":[{"type":"code"}],"text":"i"},{"type":"text","text":" onto the stack (only push during the first pass)."}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"After finishing, all elements without a next greater stay "},{"type":"text","marks":[{"type":"code"}],"text":"-1"},{"type":"text","text":"."}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" State Definition"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"code"}],"text":"stack"},{"type":"text","text":": stores indices of elements whose next greater hasnt been found."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"code"}],"text":"ans[i]"},{"type":"text","text":": the next greater element for "},{"type":"text","marks":[{"type":"code"}],"text":"nums[i]"},{"type":"text","text":"."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Transition Logic"}]},{"type":"codeBlock","attrs":{"language":null},"content":[{"type":"text","text":"n = len(nums)\\nans = [-1] * n\\nstack = []\\n\\nfor i in range(2 * n):\\n    num = nums[i % n]\\n    while stack and num > nums[stack[-1]]:\\n        ans[stack.pop()] = num\\n    if i < n:\\n        stack.append(i)\\n"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Code Implementation"}]},{"type":"codeBlock","attrs":{"language":null},"content":[{"type":"text","text":"def nextGreaterElements(self, nums: List[int]) -> List[int]:\\n        stack = []\\n        n = len(nums)\\n        nums = nums * 2\\n        res = [-1] * (n * 2)\\n        for i, num in enumerate(nums):\\n            while stack and num > nums[stack[-1]]:\\n                prev_index = stack.pop()\\n                res[prev_index] = num\\n            stack.append(i)\\n        return res[:n]"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Example Walkthrough"}]},{"type":"heading","attrs":{"level":3},"content":[{"type":"text","text":"Example 1"}]},{"type":"paragraph","content":[{"type":"text","text":"nums = [1, 2, 1]"},{"type":"hardBreak"},{"type":"text","text":"n = 3"}]},{"type":"paragraph","content":[{"type":"text","text":"Start with "},{"type":"text","marks":[{"type":"code"}],"text":"ans = [-1, -1, -1]"},{"type":"text","text":" and empty "},{"type":"text","marks":[{"type":"code"}],"text":"stack"},{"type":"text","text":"."}]},{"type":"paragraph","content":[{"type":"text","text":"Pass 1 (first loop through nums):"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i=0 (1): stack = [0]"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i=1 (2): 2 > 1  pop 0  ans[0] = 2  push 1"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i=2 (1): 1 < 2  push 2  stack = [1, 2]"}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"Pass 2 (simulate circular part):"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i=3 (1): num=1, 1 < 1  nothing"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i=4 (2): num=2, 2 > 1  pop 2  ans[2] = 2"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i=5 (1): num=1, 1 < 2  nothing"}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"Final ans = [2, -1, 2]"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Complexity"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Time:"},{"type":"text","text":" O(n)  Each index is pushed and popped at most once (even across the 2n loop)."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Space:"},{"type":"text","text":" O(n)  Stack and result array."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Why It Works"}]},{"type":"paragraph","content":[{"type":"text","text":"The "},{"type":"text","marks":[{"type":"bold"}],"text":"second pass"},{"type":"text","text":" ensures that elements near the end of the array get a chance to find a next greater among earlier elements."},{"type":"hardBreak"},{"type":"text","text":"But we never push indices during the second pass, so the stack doesnt grow infinitely."}]},{"type":"paragraph","content":[{"type":"text","text":"Its a clever linear-time trick to simulate circularity without actually modifying the array."}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" When to Recognize the Pattern"}]},{"type":"paragraph","content":[{"type":"text","text":"Youre in this pattern if you see:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Circular array + next greater/smaller element."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Wrap-around search or can search past the end."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Keywords like circularly, wrap, or repeated array."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Pattern Family"}]},{"type":"paragraph","content":[{"type":"text","text":"Monotonic Stack  "},{"type":"text","marks":[{"type":"italic"}],"text":"Next Greater Element"}]},{"type":"paragraph","content":[{"type":"text","text":"Related problems:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Next Greater Element I  basic version (non-circular)."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Daily Temperatures  distance instead of value."}]}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Online Stock Span  reverse direction."}]}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Largest Rectangle in Histogram  next smaller variant."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Variants"}]},{"type":"orderedList","attrs":{"start":1,"type":null},"content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Circular Next Smaller Element"},{"type":"text","text":"  same but reverse comparison ("},{"type":"text","marks":[{"type":"code"}],"text":"<"},{"type":"text","text":")."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Find Next Greater Distance"},{"type":"text","text":"  store "},{"type":"text","marks":[{"type":"code"}],"text":"i - prev"},{"type":"text","text":" instead of "},{"type":"text","marks":[{"type":"code"}],"text":"nums[i]"},{"type":"text","text":"."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Circular Minimum Element Distance"},{"type":"text","text":"  same concept but with min condition."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Next Greater Element in Left Direction"},{"type":"text","text":"  iterate backward."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Mnemonic Insight"}]},{"type":"blockquote","content":[{"type":"paragraph","content":[{"type":"text","text":"Think of the array twice in a row "},{"type":"hardBreak"},{"type":"text","text":"youre just giving the last few elements a second chance."}]}]},{"type":"paragraph","content":[{"type":"text","text":"Or more intuitively:"}]},{"type":"blockquote","content":[{"type":"paragraph","content":[{"type":"text","text":"Loop twice, push once, pop when warmer."}]}]}]}	https://leetcode.com/problems/next-greater-element-ii/	[{"id":1,"type":"text","content":"","placeholder":"Type \\"/\\" for commands"}]	2025-09-21 14:04:17.091216	2025-10-22 17:04:08.924773	{664,783}

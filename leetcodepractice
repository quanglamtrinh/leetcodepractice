--
-- TOC entry 270 (class 1259 OID 61325)
-- Name: practice_session_history; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.practice_session_history AS
 SELECT ce.id,
    ce.event_date,
    ce.problem_id,
    p.title AS problem_title,
    p.difficulty,
    ce.time_spent_minutes,
    ce.was_successful,
    ce.note_content
   FROM (public.calendar_events ce
     JOIN public.problems p ON ((ce.problem_id = p.id)))
  WHERE ((ce.event_type = 'practice_session'::public.event_type) AND (ce.is_archived = false))
  ORDER BY ce.event_date DESC, ce.created_at DESC;


ALTER VIEW public.practice_session_history OWNER TO leetcodeuser;

--
-- TOC entry 263 (class 1259 OID 59911)
-- Name: problem_association_summary; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.problem_association_summary AS
 SELECT pr.id AS problem_id,
    pr.title AS problem_title,
    pr.difficulty AS problem_difficulty,
    pr.concept AS problem_concept,
    count(pa.id) AS total_associations,
    count(
        CASE
            WHEN (pa.pattern_id IS NOT NULL) THEN 1
            ELSE NULL::integer
        END) AS pattern_associations,
    count(
        CASE
            WHEN (pa.variant_id IS NOT NULL) THEN 1
            ELSE NULL::integer
        END) AS variant_associations,
    string_agg(DISTINCT p.name, ', '::text) AS associated_patterns,
    string_agg(DISTINCT v.name, ', '::text) AS associated_variants,
    max(pa.updated_at) AS last_association_update
   FROM (((public.problems pr
     LEFT JOIN public.problem_associations pa ON ((pr.id = pa.problem_id)))
     LEFT JOIN public.patterns p ON ((pa.pattern_id = p.id)))
     LEFT JOIN public.variants v ON ((pa.variant_id = v.id)))
  GROUP BY pr.id, pr.title, pr.difficulty, pr.concept
  ORDER BY (count(pa.id)) DESC NULLS LAST, pr.title;


ALTER VIEW public.problem_association_summary OWNER TO leetcodeuser;

--
-- TOC entry 5346 (class 0 OID 0)
-- Dependencies: 263
-- Name: VIEW problem_association_summary; Type: COMMENT; Schema: public; Owner: leetcodeuser
--

COMMENT ON VIEW public.problem_association_summary IS 'Summary of pattern/variant associations per problem';



670	670	Next Greater Element II	Misc	medium	54.60	905831	t	{"type":"doc","content":[{"type":"heading","attrs":{"level":1},"content":[{"type":"text","text":"503. Next Greater Element II"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Pattern Family:"},{"type":"text","text":" Monotonic Stack  Next Greater Element (Circular Version)"},{"type":"hardBreak"},{"type":"text","marks":[{"type":"bold"}],"text":"Type:"},{"type":"text","text":" Decreasing Stack with Modular Traversal"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Key Insights"}]},{"type":"paragraph","content":[{"type":"text","text":"We need to find, for each element in "},{"type":"text","marks":[{"type":"code"}],"text":"nums"},{"type":"text","text":", the "},{"type":"text","marks":[{"type":"bold"}],"text":"next greater element"},{"type":"text","text":" "},{"type":"hardBreak"},{"type":"text","text":"but the array is "},{"type":"text","marks":[{"type":"bold"}],"text":"circular"},{"type":"text","text":", meaning the last element can wrap around to the first."}]},{"type":"paragraph","content":[{"type":"text","text":"For example:"},{"type":"hardBreak"},{"type":"text","marks":[{"type":"code"}],"text":"[1,2,1]"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"the first "},{"type":"text","marks":[{"type":"code"}],"text":"1"},{"type":"text","text":"  next greater is "},{"type":"text","marks":[{"type":"code"}],"text":"2"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"code"}],"text":"2"},{"type":"text","text":"  no greater  "},{"type":"text","marks":[{"type":"code"}],"text":"-1"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"the second "},{"type":"text","marks":[{"type":"code"}],"text":"1"},{"type":"text","text":"  must wrap around to find "},{"type":"text","marks":[{"typ
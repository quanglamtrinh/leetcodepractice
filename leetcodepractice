-- TOC entry 265 (class 1259 OID 61234)
-- Name: calendar_events; Type: TABLE; Schema: public; Owner: leetcodeuser
--

CREATE TABLE public.calendar_events (
    id bigint NOT NULL,
    event_type public.event_type NOT NULL,
    title character varying(255) NOT NULL,
    description text,
    event_date date NOT NULL,
    event_time time without time zone,
    duration_minutes integer,
    all_day boolean DEFAULT false,
    task_status public.task_status DEFAULT 'pending'::public.task_status,
    due_date date,
    completed_date timestamp without time zone,
    priority public.event_priority DEFAULT 'medium'::public.event_priority,
    note_content text,
    is_pinned boolean DEFAULT false,
    problem_id bigint,
    time_spent_minutes integer,
    was_successful boolean,
    content_html text,
    parent_event_id bigint,
    tags text[],
    color character varying(7),
    is_archived boolean DEFAULT false,
    reminder_minutes_before integer,
    reminder_sent boolean DEFAULT false,
    is_visible boolean DEFAULT true,
    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    updated_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    difficulty character varying(10),
    CONSTRAINT note_content_check CHECK (((event_type <> 'note'::public.event_type) OR (note_content IS NOT NULL))),
    CONSTRAINT practice_session_check CHECK (((event_type <> 'practice_session'::public.event_type) OR (problem_id IS NOT NULL))),
    CONSTRAINT task_fields_check CHECK (((event_type <> 'task'::public.event_type) OR (task_status IS NOT NULL))),
    CONSTRAINT valid_color CHECK ((((color)::text ~ '^#[0-9A-Fa-f]{6}$'::text) OR (color IS NULL))),
    CONSTRAINT valid_due_date CHECK ((due_date >= event_date))
);


ALTER TABLE public.calendar_events OWNER TO leetcodeuser;


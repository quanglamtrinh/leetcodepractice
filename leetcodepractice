--
-- TOC entry 268 (class 1259 OID 61316)
-- Name: todays_events; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.todays_events AS
 SELECT id,
    event_type,
    title,
    event_time,
    task_status,
    priority,
    color,
    problem_difficulty
   FROM public.get_events_for_day(CURRENT_DATE) get_events_for_day(id, event_type, title, event_time, task_status, priority, color, problem_difficulty);


ALTER VIEW public.todays_events OWNER TO leetcodeuser;

--
-- TOC entry 269 (class 1259 OID 61320)
-- Name: upcoming_tasks; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.upcoming_tasks AS
 SELECT id,
    title,
    description,
    event_date,
    due_date,
    task_status,
    priority,
    tags
   FROM public.calendar_events ce
  WHERE ((event_type = 'task'::public.event_type) AND (task_status <> ALL (ARRAY['completed'::public.task_status, 'cancelled'::public.task_status])) AND ((due_date >= CURRENT_DATE) AND (due_date <= (CURRENT_DATE + '7 days'::interval))) AND (is_archived = false))
  ORDER BY due_date, priority DESC;


ALTER VIEW public.upcoming_tasks OWNER TO leetcodeuser;

--
-- TOC entry 256 (class 1259 OID 59784)
-- Name: variant_performance; Type: TABLE; Schema: public; Owner: leetcodeuser
--

CREATE TABLE public.variant_performance (
    id bigint NOT NULL,
    user_id bigint DEFAULT 1,
    variant_id bigint NOT NULL,
    pattern_id bigint NOT NULL,
    total_attempts integer DEFAULT 0,
    successful_attempts integer DEFAULT 0,
    last_reviewed date,
    average_time_minutes numeric(5,2),
    difficulty_rating numeric(3,2) DEFAULT 0.5,
    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    updated_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP
);


ALTER TABLE public.variant_performance OWNER TO leetcodeuser;

--
-- TOC entry 5359 (class 0 OID 0)
-- Dependencies: 256
-- Name: TABLE variant_performance; Type: COMMENT; Schema: public; Owner: leetcodeuser
--

COMMENT ON TABLE public.variant_performance IS 'Tracks user performance metrics for each variant';


--
-- TOC entry 262 (class 1259 OID 59906)
-- Name: variant_analytics; Type: VIEW; Schema: public; Owner: leetcodeuser
--

CREATE VIEW public.variant_analytics AS
 SELECT v.id AS variant_id,
    v.name AS variant_name,
    v.use_when,
    v.notes AS variant_notes,
    p.name AS pattern_name,
    t.name AS technique_name,
    g.name AS goal_name,
    vp.total_attempts,
    vp.successful_attempts,
        CASE
            WHEN (vp.total_attempts > 0) THEN round((((vp.successful_attempts)::numeric * 100.0) / (vp.total_attempts)::numeric), 2)
            ELSE (0)::numeric
        END AS success_rate,
    vp.average_time_minutes,
    vp.difficulty_rating,
    vp.last_reviewed,
    count(pa.id) AS associated_problems_count
   FROM (((((public.variants v
     LEFT JOIN public.patterns p ON ((v.pattern_id = p.id)))
     LEFT JOIN public.techniques t ON ((v.technique_id = t.id)))
     LEFT JOIN public.goals g ON ((v.goal_id = g.id)))
     LEFT JOIN public.variant_performance vp ON ((v.id = vp.variant_id)))
     LEFT JOIN public.problem_associations pa ON ((v.id = pa.variant_id)))
  GROUP BY v.id, v.name, v.use_when, v.notes, p.name, t.name, g.name, vp.total_attempts, vp.successful_attempts, vp.average_time_minutes, vp.difficulty_rating, vp.last_reviewed
  ORDER BY vp.difficulty_rating DESC NULLS LAST, v.name;


ALTER VIEW public.variant_analytics OWNER TO leetcodeuser;

--
-- TOC entry 5360 (class 0 OID 0)
-- Dependencies: 262
-- Name: VIEW variant_analytics; Type: COMMENT; Schema: public; Owner: leetcodeuser
--

COMMENT ON VIEW public.variant_analytics IS 'Comprehensive analytics view for variant performance';



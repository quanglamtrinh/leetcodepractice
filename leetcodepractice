783	783	Daily Temperatures	Misc	medium	62.30	1998478	t	{"type":"doc","content":[{"type":"heading","attrs":{"level":1},"content":[{"type":"text","text":" 739. Daily Temperatures"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Pattern Family:"},{"type":"text","text":" Monotonic Stack  Next Greater Element Pattern"},{"type":"hardBreak"},{"type":"text","marks":[{"type":"bold"}],"text":"Type:"},{"type":"text","text":" Decreasing Stack (distance-based version)"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Key Insights"}]},{"type":"paragraph","content":[{"type":"text","text":"We need, for each day, to find "},{"type":"text","marks":[{"type":"bold"}],"text":"how many days until a warmer temperature appears"},{"type":"text","text":"."}]},{"type":"paragraph","content":[{"type":"text","text":"Brute force (nested loops) would check all future days  O(n)."},{"type":"hardBreak"},{"type":"text","text":"But we can notice this pattern:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Once a warmer day is found for an earlier day, we never need to check that earlier day again."}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"This means we can store "},{"type":"text","marks":[{"type":"bold"}],"text":"unresolved days"},{"type":"text","text":" (indices) in a stack and resolve them efficiently when a warmer temperature comes."}]},{"type":"paragraph","content":[{"type":"text","text":" Each index is pushed and popped at most once  O(n) time."}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Why / When to Use"}]},{"type":"paragraph","content":[{"type":"text","text":"Use this pattern when:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"You need to find the "},{"type":"text","marks":[{"type":"bold"}],"text":"next greater"},{"type":"text","text":" (or smaller) element "},{"type":"text","marks":[{"type":"bold"}],"text":"to the right"},{"type":"text","text":" (or left)."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Youre looking for the "},{"type":"text","marks":[{"type":"bold"}],"text":"first future value"},{"type":"text","text":" that satisfies a condition (e.g., warmer day, higher price)."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"You can express the relation as pop while the condition is broken."}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"Keywords to recognize this pattern:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"italic"}],"text":"Next greater element"},{"type":"text","text":", "},{"type":"text","marks":[{"type":"italic"}],"text":"next smaller element"},{"type":"text","text":", "},{"type":"text","marks":[{"type":"italic"}],"text":"days to wait"},{"type":"text","text":", "},{"type":"text","marks":[{"type":"italic"}],"text":"future warmer/higher/lower value"},{"type":"text","text":"."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Technique Summary"}]},{"type":"paragraph","content":[{"type":"text","text":"We maintain a "},{"type":"text","marks":[{"type":"bold"}],"text":"monotonic decreasing stack"},{"type":"text","text":" of indices."},{"type":"hardBreak"},{"type":"text","text":"When we encounter a new temperature thats "},{"type":"text","marks":[{"type":"bold"}],"text":"higher"},{"type":"text","text":" than the temperature at the index on top of the stack,"},{"type":"hardBreak"},{"type":"text","text":"we pop that index and compute how many days it took to find a warmer one."}]},{"type":"paragraph","content":[{"type":"text","text":"We keep doing this until the stack is either empty or the current temperature is not higher than the top."}]},{"type":"paragraph","content":[{"type":"text","text":"Then we push the current days index onto the stack."}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" State Definition"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"code"}],"text":"stack"},{"type":"text","text":": stores indices of days whose warmer day hasnt been found yet."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"code"}],"text":"ans[i]"},{"type":"text","text":": the number of days to wait until a warmer temperature for day "},{"type":"text","marks":[{"type":"code"}],"text":"i"},{"type":"text","text":"."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Transition Logic"}]},{"type":"paragraph","content":[{"type":"text","text":"For each index "},{"type":"text","marks":[{"type":"code"}],"text":"i"},{"type":"text","text":" in "},{"type":"text","marks":[{"type":"code"}],"text":"temperatures"},{"type":"text","text":":"}]},{"type":"orderedList","attrs":{"start":1,"type":null},"content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"While stack is not empty and "},{"type":"text","marks":[{"type":"code"}],"text":"temperatures[i] > temperatures[stack[-1]]"},{"type":"text","text":":"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Pop the top index "},{"type":"text","marks":[{"type":"code"}],"text":"prev"},{"type":"text","text":"."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Set "},{"type":"text","marks":[{"type":"code"}],"text":"ans[prev] = i - prev"},{"type":"text","text":" (difference between indices = days waited)."}]}]}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Push current index "},{"type":"text","marks":[{"type":"code"}],"text":"i"},{"type":"text","text":" onto stack."}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"At the end, any remaining indices have no warmer day, so "},{"type":"text","marks":[{"type":"code"}],"text":"ans[i] = 0"},{"type":"text","text":"."}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Code Implementation"}]},{"type":"codeBlock","attrs":{"language":null},"content":[{"type":"text","text":"class Solution:\\n    def dailyTemperatures(self, temperatures: List[int]) -> List[int]:\\n        n = len(temperatures)\\n        ans = [0] * n\\n        stack = []  # stores indices\\n        \\n        for i, temp in enumerate(temperatures):\\n            while stack and temp > temperatures[stack[-1]]:\\n                prev = stack.pop()\\n                ans[prev] = i - prev\\n            stack.append(i)\\n        \\n        return ans\\n"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Example Walkthrough"}]},{"type":"paragraph","content":[{"type":"text","text":"temperatures = [73, 74, 75, 71, 69, 72, 76, 73]"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 0, temp = 73  push  stack = [0]"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 1, temp = 74  74 > 73  pop 0  ans[0] = 1  push 1"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 2, temp = 75  75 > 74  pop 1  ans[1] = 1  push 2"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 3, temp = 71  push  stack = [2, 3]"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 4, temp = 69  push  stack = [2, 3, 4]"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 5, temp = 72  pop 4  ans[4] = 1; pop 3  ans[3] = 2  push 5"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 6, temp = 76  pop 5  ans[5] = 1; pop 2  ans[2] = 4  push 6"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"i = 7, temp = 73  push  stack = [6, 7]"}]}]}]},{"type":"paragraph","content":[{"type":"text","text":"Final result:"},{"type":"hardBreak"},{"type":"text","marks":[{"type":"code"}],"text":"ans = [1, 1, 4, 2, 1, 1, 0, 0]"}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Complexity"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Time:"},{"type":"text","text":" O(n)  each element is pushed and popped once."}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Space:"},{"type":"text","text":" O(n)  stack stores indices."}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Why It Works"}]},{"type":"paragraph","content":[{"type":"text","text":"The stack acts as a "},{"type":"text","marks":[{"type":"bold"}],"text":"memory"},{"type":"text","text":" of unresolved temperatures waiting for something warmer."},{"type":"hardBreak"},{"type":"text","text":"When a warmer day appears, all cooler days before it are resolved in one go."}]},{"type":"paragraph","content":[{"type":"text","text":"Each index is processed only twice  once when pushed, once when popped."},{"type":"hardBreak"},{"type":"text","text":"This ensures linear time performance."}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" When to Recognize the Pattern"}]},{"type":"paragraph","content":[{"type":"text","text":"Youre in this pattern if you see:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Next day with higher/lower value"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Find nearest greater element"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"How many steps until condition met"}]}]},{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Span, wait, or distance phrasing"}]}]}]},{"type":"horizontalRule"},{"type":"heading","attrs":{"level":2},"content":[{"type":"text","text":" Pattern Family"}]},{"type":"paragraph","content":[{"type":"text","marks":[{"type":"bold"}],"text":"Monotonic Stack Pattern (Next Greater Element Family)"}]},{"type":"paragraph","content":[{"type":"text","text":"Related problems:"}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"496. Next Greater Element I  find the next greater "},{"type":"text","marks":[{"type":"italic"}],"text":"value"},{"type":"text","text":" for each element."}]}]},{"type":"listItem","content":[{"type":"paragraph"}]}]},{"type":"orderedList","attrs":{"start":503,"type":null},"content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Next Greater Element II  same, but circular array."}]}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph"}]}]},{"type":"orderedList","attrs":{"start":901,"type":null},"content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Online Stock Span  count how many consecutive previous days  today."}]}]}]},{"type":"bulletList","content":[{"type":"listItem","content":[{"type":"paragraph"}]}]},{"type":"orderedList","attrs":{"start":84,"type":null},"content":[{"type":"listItem","content":[{"type":"paragraph","content":[{"type":"text","text":"Largest Rectangle in Histogram  use stack to find nearest smaller boundaries."}]}]}]